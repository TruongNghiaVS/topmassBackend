@page
@using crmHuman.Pages
@model NTDModel
@{
    ViewData["Title"] = Model.TitlePage;
}
<div class="pagetitle">
    <h1>@Model.TitlePage</h1>
    <nav>
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a href="/">Trang chủ</a></li>
            <li class="breadcrumb-item"><a href="/Employee">Nhân viên</a></li>
            <li class="breadcrumb-item active">Danh sách</li>
        </ol>
    </nav>
</div><!-- End Page Title -->

<section class="section">
    <div class="row">
        <div class="col-lg-12">
            <div class="card">
                <div class="card-body">

                   @{
                        Html.RenderPartial("SearchPage/_formSearchNTD", Model);
                   }
                    @{
                        var columnText = Model.TableColumnText;
                     

                    }

                    <table class="table  table-striped">
                        <thead>
                            <tr>
                                @foreach (var item in columnText)
                                {
                                    <th>
                                        @item
                                    </th>
                                }
                            </tr>
                        </thead>
                        <tbody>
                           



                        </tbody>
                    </table>
                   @*  @{
                        Html.RenderPartial("formSearch/pagination", Model);
                    } *@


                </div>
            </div>

        </div>
    </div>
</section>

<div class="modal fade" id="formModal" tabindex="-1">
    <div class="modal-dialog  modal-dialog-centered modal-lg">
        <div class="modal-content" id="contentModal">
        </div>
    </div>
</div><!-- End Large Modal-->
